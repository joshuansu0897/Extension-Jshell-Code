{
    "name": "Jshell",
    "displayName": "jsh",
    "description": "Language Support for Jshell (JavaShell)",
    "version": "1.0.4",
    "publisher": "joshuansu",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jshell",
                "aliases": [
                    "jshell",
                    "jshell"
                ],
                "extensions": [
                    ".jsh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jshell",
                "scopeName": "source.java",
                "path": "./syntaxes/java.tmLanguage"
            }
        ]
    }
}